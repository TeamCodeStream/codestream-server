{
	"$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
	"version": "1.0.3",
	"manifestVersion": "1.12",
	"id": "{botId}",
	"packageName": "codestream.codestream-eu",
	"name": { "short": "CodeStream EU", "full": "" },
	"developer": {
		"name": "CodeStream",
		"mpnId": "",
		"websiteUrl": "https://newrelic.com/codestream",
		"privacyUrl": "https://newrelic.com/termsandconditions/privacy",
		"termsOfUseUrl": "https://newrelic.com/termsandconditions/terms"
	},
	"description": {
		"short": "CodeStream is an extension to your IDE that works with your existing tools.",
		"full": "CodeStream EU is intended for use with CodeStream organizations in the EU region. For CodeStream organizations in the US region use the CodeStream app instead. Shift left by making code performance, code quality and code discussion part of the earliest stages of the development process."
	},
	"icons": { "outline": "outline.png", "color": "color.png" },
	"accentColor": "#00CE7C",
	"bots": [
		{
			"botId": "{botId}",
			"scopes": ["personal", "team"],
			"commandLists": [
				{
					"commands": [
						{ "title": "help", "description": "Get help using CodeStream" },
						{ "title": "signin", "description": "Sign in to CodeStream" },
						{ "title": "signout", "description": "Sign out from CodeStream" }
					],
					"scopes": ["personal"]
				},
				{
					"commands": [
						{ "title": "help", "description": "Get help using CodeStream" },
						{
							"title": "connect",
							"description": "Connect this channel to CodeStream"
						},
						{
							"title": "disconnect",
							"description": "Disconnect this channel from CodeStream"
						}
					],
					"scopes": ["team"]
				}
			],
			"isNotificationOnly": false,
			"supportsCalling": false,
			"supportsVideo": false,
			"supportsFiles": false
		}
	],
	"validDomains": ["csaction.codestream.us", "www.codestream.com", "*.codestream.us"],
	"authorization": { "permissions": { "resourceSpecific": [] } }
}
