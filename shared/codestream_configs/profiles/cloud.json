		"local": {},
		"pi": {},
		"prod": {
			"inboundEmailDirectory": "${HOME}/codestream-mail/inbound/web/new"
		},
		"qa": {
			"inboundEmailDirectory": "${HOME}/codestream-mail/inbound/web/new"
		}
	},
